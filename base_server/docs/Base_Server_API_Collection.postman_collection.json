                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.user_id) {",
                  "        pm.collectionVariables.set('user_id', response.user_id);",
                  "    }",
                  "    pm.test('회원가입 성공', function () {",
